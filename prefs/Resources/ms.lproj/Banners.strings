<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>BANNERS</key>
	<string>Sepanduk</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Aktifkan</string>

	<!-- Switch to toggle showing an album art icon. -->
	<key>ALBUM_ART_ICON</key>
	<string>Album Ikon</string>

	<!-- Explanation of the album art icon switch. -->
	<key>ALBUM_ART_ICON_EXPLANATION</key>
	<string>Menunjukkan album lagu yang dimainkan dalam pemberitahuan perubahan lagu yang ditunjukkan oleh tweak seperti NowListening.</string>

	<!-- Switches to toggle a gradient background, round corners, and displaying a shadow below the text. -->
	<key>GRADIENT</key>
	<string>Kecerunan</string>
	<key>ROUND_CORNERS</key>
	<string>Penjuru Bulat</string>
	<key>TEXT_SHADOW</key>
	<string>Bayang Teks</string>

	<!-- Warning message explaining that the shadows can't be used due to an incompatibility. -->
	<key>TEXT_SHADOWS_INCOMPATIBILITY_WARNING</key>
	<string>Bayang teks tidak serasi dengan TinyBar.</string>

	<!-- Label for background opacity (transparency), color intensity, and grayscale intensity sliders. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Kelegapan Latar Belakang</string>
	<key>COLOR_INTENSITY</key>
	<string>Keamatan Warna</string>
	<key>GRAYSCALE_INTENSITY</key>
	<string>Keamatan Kelabu</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Sembunyikan Pilihan</string>

	<!-- Switch to toggle hiding the icon on a banner. -->
	<key>HIDE_ICON</key>
	<string>Sembunyikan Ikon</string>

	<!-- Switch to toggle hiding the grabber on a banner. -->
	<key>HIDE_GRABBER</key>
	<string>Sembunyikan Pemegang</string>
</dict>
</plist>
