<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>NOTIFICATION_CENTER</key>
	<string>Pusat Notifikasi</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Aktifkan</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Pudar Atas dan Bawah</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Tambah efek pudar halus pada bahagian atas dan bawah untuk senarai notifikasi.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Kelegapan Latar Belakang</string>

	<!-- Label explaining a respring is requierd to apply settings. -->
	<key>RESPRING_EXPLANATION</key>
	<string>Respring untuk mengubah seting.</string>

	<!-- Button to perform a respring. -->
	<key>RESPRING</key>
	<string>Respring</string>
</dict>
</plist>
