<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>NOTIFICATION_CENTER</key>
	<string>Berichtencentrum</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Ingeschakeld</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Vervaag boven- en onderkant</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Voegt een subtiel vervagend effect toe aan de boven- en onderkant van de lijst met meldingen.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Achtergrond dekking</string>

	<!-- Label explaining a respring is requierd to apply settings. -->
	<key>RESPRING_EXPLANATION</key>
	<string>Herlaad het SpringBoard om deze instellingen toe te passen.</string>

	<!-- Button to perform a respring. -->
	<key>RESPRING</key>
	<string>Herlaad SpringBoard</string>
</dict>
</plist>
