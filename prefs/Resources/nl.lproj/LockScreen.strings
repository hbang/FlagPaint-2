<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>LOCK_SCREEN</key>
	<string>Toegangsscherm</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Ingeschakeld</string>

	<!-- Switch to toggle using a gradient or solid background. -->
	<key>GRADIENT</key>
	<string>Gradient</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Vervaag boven- en onderkant</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Voegt een subtiel vervagend effect toe aan de boven- en onderkant van de lijst met meldingen.</string>

	<!-- Switch to disable notifications from dimming. -->
	<key>DISABLE_DIMMING</key>
	<string>Niet dimmen</string>

	<!-- Text explaining what the Disable Dimming switch does. -->
	<key>DISABLE_DIMMING_EXPLANATION</key>
	<string>Zorgt ervoor dat niet meer alle meldingen behalve de eerste worden gedimd.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Achtergrond dekking</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Onderdelen verbergen</string>

	<!-- Switch to toggle hiding the “slide to view” text below a notification. -->
	<key>HIDE_SLIDE_ACTION</key>
	<string>Verberg “Schuif om te”</string>
</dict>
</plist>
