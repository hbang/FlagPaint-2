<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>LOCK_SCREEN</key>
	<string>Låst skjerm</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Aktivert</string>

	<!-- Switch to toggle using a gradient or solid background. -->
	<key>GRADIENT</key>
	<string>Gradient</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Fade topp og bunn</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Legger til en subtil fade effekt til toppen og bunnen av notifikasjons listen.</string>

	<!-- Switch to disable notifications from dimming. -->
	<key>DISABLE_DIMMING</key>
	<string>Deaktiver dimming</string>

	<!-- Text explaining what the Disable Dimming switch does. -->
	<key>DISABLE_DIMMING_EXPLANATION</key>
	<string>Deaktiverer standard dimming av notifikasjoner.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Bakgrunn gjennomsiktighet</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Skjul elementer</string>

	<!-- Switch to toggle hiding the “slide to view” text below a notification. -->
	<key>HIDE_SLIDE_ACTION</key>
	<string>Skjul “skyv for å”</string>
</dict>
</plist>
