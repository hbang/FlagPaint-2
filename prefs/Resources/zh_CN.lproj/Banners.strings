<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>BANNERS</key>
	<string>横幅</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>开启</string>

	<!-- Switch to toggle showing an album art icon. -->
	<key>ALBUM_ART_ICON</key>
	<string>专辑图片</string>

	<!-- Explanation of the album art icon switch. -->
	<key>ALBUM_ART_ICON_EXPLANATION</key>
	<string>显示例如NowListening插件的当前播放专辑图片。</string>

	<!-- Switches to toggle a gradient background, round corners, and displaying a shadow below the text. -->
	<key>GRADIENT</key>
	<string>梯度</string>
	<key>ROUND_CORNERS</key>
	<string>圆角</string>
	<key>TEXT_SHADOW</key>
	<string>文字阴影</string>

	<!-- Warning message explaining that the shadows can't be used due to an incompatibility. -->
	<key>TEXT_SHADOWS_INCOMPATIBILITY_WARNING</key>
	<string>文字阴影功能与TinyBar不兼容。</string>

	<!-- Label for background opacity (transparency), color intensity, and grayscale intensity sliders. -->
	<key>BACKGROUND_OPACITY</key>
	<string>背景模糊</string>
	<key>COLOR_INTENSITY</key>
	<string>颜色强度</string>
	<key>GRAYSCALE_INTENSITY</key>
	<string>灰度</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>隐藏项目</string>

	<!-- Switch to toggle hiding the icon on a banner. -->
	<key>HIDE_ICON</key>
	<string>隐藏图标</string>

	<!-- Switch to toggle hiding the grabber on a banner. -->
	<key>HIDE_GRABBER</key>
	<string>隐藏下拉条</string>
</dict>
</plist>
