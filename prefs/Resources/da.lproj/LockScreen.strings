<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>LOCK_SCREEN</key>
	<string>Låseskærm</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Aktiveret</string>

	<!-- Switch to toggle using a gradient or solid background. -->
	<key>GRADIENT</key>
	<string>Farveovergang</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Fader kanterne i top og bund gradvist</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Tilføjer en nuanceret fade effekt til toppen og bunden af notifikations-listen.</string>

	<!-- Switch to disable notifications from dimming. -->
	<key>DISABLE_DIMMING</key>
	<string>Deaktiver Lys-dæmpning</string>

	<!-- Text explaining what the Disable Dimming switch does. -->
	<key>DISABLE_DIMMING_EXPLANATION</key>
	<string>Deaktivere standard lys-dæmpning af alle, pånær den første notifikation der bliver lys-dæmpet.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Baggrund gennemsigtighed</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Skjul Elementer</string>

	<!-- Switch to toggle hiding the “slide to view” text below a notification. -->
	<key>HIDE_SLIDE_ACTION</key>
	<string>Skjul “Skub for at vise”</string>
</dict>
</plist>
