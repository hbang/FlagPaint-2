<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>NOTIFICATION_CENTER</key>
	<string>Centro de Notificaciones</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Activo</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Difuminar Parte Superior e Inferior</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Agrega un ligero efecto difuminado en la parte superior e inferior de la lista de notificaciones.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Opacidad del Fondo</string>

	<!-- Label explaining a respring is requierd to apply settings. -->
	<key>RESPRING_EXPLANATION</key>
	<string>Reinicia el Springboard para aplicar estos cambios</string>

	<!-- Button to perform a respring. -->
	<key>RESPRING</key>
	<string>Hacer un Respring</string>
</dict>
</plist>
