<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>LOCK_SCREEN</key>
	<string>Lock Screen</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Nyala</string>

	<!-- Switch to toggle using a gradient or solid background. -->
	<key>GRADIENT</key>
	<string>Gradien</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Lunturkan Atas dan Bawah</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Menambahkan efek luntur yang halus ke bagian atas dan bawah daftar notifikasi.</string>

	<!-- Switch to disable notifications from dimming. -->
	<key>DISABLE_DIMMING</key>
	<string>Matikan efek redup</string>

	<!-- Text explaining what the Disable Dimming switch does. -->
	<key>DISABLE_DIMMING_EXPLANATION</key>
	<string>Menghapus efek redup dari semua notifikasi kecuali yang pertama.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Ketebalan latar belakang</string>

	<!-- Title for the "Hide Elements" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Sembunyikan Elemen</string>

	<!-- Switch to toggle hiding the “slide to view” text below a notification. -->
	<key>HIDE_SLIDE_ACTION</key>
	<string>Sembunyikan “Slide To”</string>
</dict>
</plist>
