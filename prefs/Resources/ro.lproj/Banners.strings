<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>BANNERS</key>
	<string>Bannere</string>

	<!-- Switch to toggle tinting. -->
	<key>ENABLED</key>
	<string>Activat</string>

	<!-- Switch to toggle showing an album art icon. -->
	<key>ALBUM_ART_ICON</key>
	<string>Simbol album</string>

	<!-- Explanation of the album art icon switch. -->
	<key>ALBUM_ART_ICON_EXPLANATION</key>
	<string>Afiseaza imaginea albumului cantecului curent in notificari livrate de tweak-uri precum NowListening.</string>

	<!-- Switches to toggle a gradient background, round corners, and displaying a shadow below the text. -->
	<key>GRADIENT</key>
	<string>Gradient</string>
	<key>ROUND_CORNERS</key>
	<string>Colturi rotunjite</string>
	<key>TEXT_SHADOW</key>
	<string>Umbra text</string>

	<!-- Warning message explaining that the shadows can't be used due to an incompatibility. -->
	<key>TEXT_SHADOWS_INCOMPATIBILITY_WARNING</key>
	<string>Umbrele nu sunt compatibile cu TinyBar.</string>

	<!-- Label for background opacity (transparency), color intensity, and grayscale intensity sliders. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Transparenta fundal</string>
	<key>COLOR_INTENSITY</key>
	<string>Intensitate culoare</string>
	<key>GRAYSCALE_INTENSITY</key>
	<string>Intensitate griuri</string>

	<!-- Title for the "Ascunde elemente" section. -->
	<key>HIDE_ELEMENTS_TITLE</key>
	<string>Ascunde elemente</string>

	<!-- Switch to toggle hiding the icon on a banner. -->
	<key>HIDE_ICON</key>
	<string>Ascunde icoana</string>

	<!-- Switch to toggle hiding the grabber on a banner. -->
	<key>HIDE_GRABBER</key>
	<string>Ascunde Grabber</string>
</dict>
</plist>
