<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>NOTIFICATION_CENTER</key>
	<string>Notification Center</string>

	<!-- Switch to toggle fading the top and bottom. -->
	<key>FADE_EDGES</key>
	<string>Fade Top and Bottom</string>

	<!-- Text explaining what the fading feature does. -->
	<key>FADE_EDGES_EXPLANATION</key>
	<string>Adds a subtle fade effect to the top and bottom of the notification list.</string>

	<!-- Label for a background opacity (transparency) slider. -->
	<key>BACKGROUND_OPACITY</key>
	<string>Background Opacity</string>

	<!-- Label explaining a respring is requierd to apply settings. -->
	<key>RESPRING_EXPLANATION</key>
	<string>Respring to apply these settings.</string>

	<!-- Button to perform a respring. -->
	<key>RESPRING</key>
	<string>Respring</string>
</dict>
</plist>
